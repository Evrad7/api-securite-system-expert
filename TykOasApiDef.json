{
  "servers": [
    {
      "url": "{protocol}://{host}:{port}",
      "variables": {
        "protocol": {
          "enum": ["http", "https"],
          "default": "http"
        },
        "host": {
          "default": "13.36.102.183",
          "description": "adresse ip ou nom de domaine"
        },
        "port": {
          "default": "8080",
          "description": "port de communication"
        }
      }
    }
  ],
  "info": {
    "title": "expert-system",
    "version": "1.0.0"
  },
  "openapi": "3.0.3",
  "x-tyk-api-gateway": {
    "middleware": {
      "global": {
        "cache": {
          "enabled": true,
          "timeout": 500
        },
        "cors": {
          "enabled": true,
          "allowedOrigins": ["*"]
        }
      },
      "operations": {
        "home-page": {
          "rateLimit": {
            "enabled": true,
            "per": 60,
            "rate": 60
          },
          "requestSizeLimit": {
            "enabled": true,
            "value": 100
          },
          "mockResponse": {
            "enabled": true,
            "fromOASExamples": {
              "enabled": true,
              "exampleName": "default-example"
            }
          }
        },
        "search": {
          "rateLimit": {
            "enabled": true,
            "per": 60,
            "rate": 60
          },
          "requestSizeLimit": {
            "enabled": true,
            "value": 100
          },
          "cache": {
            "enabled": true,
            "cacheResponseCodes": [200]
          },
          "mockResponse": {
            "enabled": true,
            "fromOASExamples": {
              "enabled": true
            }
          }
        }
      }
    },
    "authConfigs": {
      "basic_auth": {
        "enabled": true
      }
    },
    "info": {
      "name": "expertSystem",
      "state": {
        "active": true
      }
    },
    "upstream": {
      "url": "http://httpbin.org"
    },
    "server": {
      "listenPath": {
        "value": "/"
      }
    }
  },
  "paths": {
    "/": {
      "get": {
        "operationId": "home-page",
        "description": "Home page",
        "responses": {
          "200": {
            "description": "test OK",
            "content": {
              "application/json": {
                "examples": {
                  "default-example": {
                    "value": "welcome to Expert System"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/search": {
      "get": {
        "operationId": "search",
        "summary": "Recherche de sujets",
        "description": "Permet de rechercher un sujet par mot-clé.",
        "parameters": [
          {
            "name": "query",
            "in": "query",
            "description": "Mot-clé à rechercher",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Résultat de recherche.",
            "headers": {},
            "content": {
              "application/json": {
                "example": {
                  "results": {
                    "title": "voiture",
                    "description": "Nostrud ipsum dolore consequat id quis culpa. Aliquip excepteur magna elit laborum veniam amet. Deserunt in nostrud id magna aliqua eu irure consectetur consectetur sint. Sit cillum minim eiusmod quis in cupidatat ut duis qui tempor amet ut.",
                    "url": "https://www.autokoo.com/marque"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
